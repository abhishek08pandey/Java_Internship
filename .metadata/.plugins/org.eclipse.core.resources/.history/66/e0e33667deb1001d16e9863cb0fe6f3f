package com.OneRivet.task;

import java.util.Scanner;

public class PrimeNumber {

	public static void main(String[] args) {
		int stop = 0;
		int firstNum;
		int lastNum;

		Scanner scan = new Scanner(System.in);
		System.out.println("Enter Fist number");
		firstNum = scan.nextInt();
		System.out.println("Enter Last number");
		lastNum = scan.nextInt();

		for (int i = firstNum; i <= lastNum; i++) {
			if(primeNumber(firstNum))
		}
	}
	
	public static void primeNumber(int firstNum, int lastNum) {
		for (int j = firstNum; j < lastNum; j++) {

			if (j %  == 0) {
				System.out.println(j + " :is not Prime Number");
			} else {
				System.out.println(j + " :is Prime Number");
			}
		}
	}
}